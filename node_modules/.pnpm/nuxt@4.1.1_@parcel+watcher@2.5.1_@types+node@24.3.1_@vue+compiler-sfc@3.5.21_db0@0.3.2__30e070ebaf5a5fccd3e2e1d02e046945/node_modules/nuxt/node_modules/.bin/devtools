#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/amandahusum/waystar-sanity/node_modules/.pnpm/@nuxt+devtools@2.6.3_vite@7.1.9_@types+node@24.3.1_jiti@2.5.1_sass@1.92.1_terser@5.44.0_61bd8353607ff79a6204ea8496ee57cb/node_modules/@nuxt/devtools/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/@nuxt+devtools@2.6.3_vite@7.1.9_@types+node@24.3.1_jiti@2.5.1_sass@1.92.1_terser@5.44.0_61bd8353607ff79a6204ea8496ee57cb/node_modules/@nuxt/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/@nuxt+devtools@2.6.3_vite@7.1.9_@types+node@24.3.1_jiti@2.5.1_sass@1.92.1_terser@5.44.0_61bd8353607ff79a6204ea8496ee57cb/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/amandahusum/waystar-sanity/node_modules/.pnpm/@nuxt+devtools@2.6.3_vite@7.1.9_@types+node@24.3.1_jiti@2.5.1_sass@1.92.1_terser@5.44.0_61bd8353607ff79a6204ea8496ee57cb/node_modules/@nuxt/devtools/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/@nuxt+devtools@2.6.3_vite@7.1.9_@types+node@24.3.1_jiti@2.5.1_sass@1.92.1_terser@5.44.0_61bd8353607ff79a6204ea8496ee57cb/node_modules/@nuxt/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/@nuxt+devtools@2.6.3_vite@7.1.9_@types+node@24.3.1_jiti@2.5.1_sass@1.92.1_terser@5.44.0_61bd8353607ff79a6204ea8496ee57cb/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../@nuxt/devtools/cli.mjs" "$@"
else
  exec node  "$basedir/../../../@nuxt/devtools/cli.mjs" "$@"
fi
