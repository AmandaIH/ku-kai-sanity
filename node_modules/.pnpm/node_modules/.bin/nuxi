#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/amandahusum/waystar-sanity/node_modules/.pnpm/nuxt@4.1.1_@parcel+watcher@2.5.1_@types+node@24.3.1_@vue+compiler-sfc@3.5.21_db0@0.3.2__30e070ebaf5a5fccd3e2e1d02e046945/node_modules/nuxt/bin/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/nuxt@4.1.1_@parcel+watcher@2.5.1_@types+node@24.3.1_@vue+compiler-sfc@3.5.21_db0@0.3.2__30e070ebaf5a5fccd3e2e1d02e046945/node_modules/nuxt/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/nuxt@4.1.1_@parcel+watcher@2.5.1_@types+node@24.3.1_@vue+compiler-sfc@3.5.21_db0@0.3.2__30e070ebaf5a5fccd3e2e1d02e046945/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/amandahusum/waystar-sanity/node_modules/.pnpm/nuxt@4.1.1_@parcel+watcher@2.5.1_@types+node@24.3.1_@vue+compiler-sfc@3.5.21_db0@0.3.2__30e070ebaf5a5fccd3e2e1d02e046945/node_modules/nuxt/bin/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/nuxt@4.1.1_@parcel+watcher@2.5.1_@types+node@24.3.1_@vue+compiler-sfc@3.5.21_db0@0.3.2__30e070ebaf5a5fccd3e2e1d02e046945/node_modules/nuxt/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/nuxt@4.1.1_@parcel+watcher@2.5.1_@types+node@24.3.1_@vue+compiler-sfc@3.5.21_db0@0.3.2__30e070ebaf5a5fccd3e2e1d02e046945/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
else
  exec node  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
fi
