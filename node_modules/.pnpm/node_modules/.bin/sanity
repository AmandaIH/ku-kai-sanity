#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/amandahusum/waystar-sanity/node_modules/.pnpm/sanity@4.7.0_@emotion+is-prop-valid@1.2.2_@portabletext+sanity-bridge@1.1.8_@sanity+sch_85a0c745df7ba54f0c0967f5b6a9232e/node_modules/sanity/bin/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/sanity@4.7.0_@emotion+is-prop-valid@1.2.2_@portabletext+sanity-bridge@1.1.8_@sanity+sch_85a0c745df7ba54f0c0967f5b6a9232e/node_modules/sanity/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/sanity@4.7.0_@emotion+is-prop-valid@1.2.2_@portabletext+sanity-bridge@1.1.8_@sanity+sch_85a0c745df7ba54f0c0967f5b6a9232e/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/amandahusum/waystar-sanity/node_modules/.pnpm/sanity@4.7.0_@emotion+is-prop-valid@1.2.2_@portabletext+sanity-bridge@1.1.8_@sanity+sch_85a0c745df7ba54f0c0967f5b6a9232e/node_modules/sanity/bin/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/sanity@4.7.0_@emotion+is-prop-valid@1.2.2_@portabletext+sanity-bridge@1.1.8_@sanity+sch_85a0c745df7ba54f0c0967f5b6a9232e/node_modules/sanity/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/sanity@4.7.0_@emotion+is-prop-valid@1.2.2_@portabletext+sanity-bridge@1.1.8_@sanity+sch_85a0c745df7ba54f0c0967f5b6a9232e/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sanity/bin/sanity" "$@"
else
  exec node  "$basedir/../sanity/bin/sanity" "$@"
fi
