#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/amandahusum/waystar-sanity/node_modules/.pnpm/@sanity+import@3.38.3_@types+react@19.1.12/node_modules/@sanity/import/src/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/@sanity+import@3.38.3_@types+react@19.1.12/node_modules/@sanity/import/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/@sanity+import@3.38.3_@types+react@19.1.12/node_modules/@sanity/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/@sanity+import@3.38.3_@types+react@19.1.12/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/amandahusum/waystar-sanity/node_modules/.pnpm/@sanity+import@3.38.3_@types+react@19.1.12/node_modules/@sanity/import/src/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/@sanity+import@3.38.3_@types+react@19.1.12/node_modules/@sanity/import/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/@sanity+import@3.38.3_@types+react@19.1.12/node_modules/@sanity/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/@sanity+import@3.38.3_@types+react@19.1.12/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@sanity/import/src/cli.js" "$@"
else
  exec node  "$basedir/../@sanity/import/src/cli.js" "$@"
fi
