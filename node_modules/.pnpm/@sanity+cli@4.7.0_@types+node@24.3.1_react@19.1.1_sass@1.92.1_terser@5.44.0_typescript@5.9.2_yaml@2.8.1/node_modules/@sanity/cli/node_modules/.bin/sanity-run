#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/amandahusum/waystar-sanity/node_modules/.pnpm/@sanity+runtime-cli@10.6.0_@types+node@24.3.1_debug@4.4.1_sass@1.92.1_terser@5.44.0_typescript@5.9.2_yaml@2.8.1/node_modules/@sanity/runtime-cli/bin/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/@sanity+runtime-cli@10.6.0_@types+node@24.3.1_debug@4.4.1_sass@1.92.1_terser@5.44.0_typescript@5.9.2_yaml@2.8.1/node_modules/@sanity/runtime-cli/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/@sanity+runtime-cli@10.6.0_@types+node@24.3.1_debug@4.4.1_sass@1.92.1_terser@5.44.0_typescript@5.9.2_yaml@2.8.1/node_modules/@sanity/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/@sanity+runtime-cli@10.6.0_@types+node@24.3.1_debug@4.4.1_sass@1.92.1_terser@5.44.0_typescript@5.9.2_yaml@2.8.1/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/amandahusum/waystar-sanity/node_modules/.pnpm/@sanity+runtime-cli@10.6.0_@types+node@24.3.1_debug@4.4.1_sass@1.92.1_terser@5.44.0_typescript@5.9.2_yaml@2.8.1/node_modules/@sanity/runtime-cli/bin/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/@sanity+runtime-cli@10.6.0_@types+node@24.3.1_debug@4.4.1_sass@1.92.1_terser@5.44.0_typescript@5.9.2_yaml@2.8.1/node_modules/@sanity/runtime-cli/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/@sanity+runtime-cli@10.6.0_@types+node@24.3.1_debug@4.4.1_sass@1.92.1_terser@5.44.0_typescript@5.9.2_yaml@2.8.1/node_modules/@sanity/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/@sanity+runtime-cli@10.6.0_@types+node@24.3.1_debug@4.4.1_sass@1.92.1_terser@5.44.0_typescript@5.9.2_yaml@2.8.1/node_modules:/Users/amandahusum/waystar-sanity/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@sanity+runtime-cli@10.6.0_@types+node@24.3.1_debug@4.4.1_sass@1.92.1_terser@5.44.0_typescript@5.9.2_yaml@2.8.1/node_modules/@sanity/runtime-cli/bin/run.js" "$@"
else
  exec node  "$basedir/../../../../../../@sanity+runtime-cli@10.6.0_@types+node@24.3.1_debug@4.4.1_sass@1.92.1_terser@5.44.0_typescript@5.9.2_yaml@2.8.1/node_modules/@sanity/runtime-cli/bin/run.js" "$@"
fi
