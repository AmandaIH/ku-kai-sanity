{
  "buildCommand": "corepack enable && corepack prepare pnpm@8.15.6 --activate && cd apps/web && pnpm build",
  "installCommand": "npm install -g pnpm@8.15.6 && pnpm install --no-optional --config.optional=false",
  "framework": "nuxtjs",
  "functions": {
    "apps/web/app/**": {
      "runtime": "nodejs20.x"
    }
  },
  "env": {
    "npm_config_optional": "false",
    "npm_config_omit": "optional",
    "PNPM_SKIP_LOCKFILE": "false",
    "NODE_VERSION": "20",
    "VERCEL_NODE_VERSION": "20"
  }
}

